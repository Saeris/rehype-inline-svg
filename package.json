{
  "name": "@saeris/rehype-inline-svg",
  "version": "0.0.4",
  "description": "A rehype plugin that inlines and optimizes SVG images",
  "author": "Drake Costa <drake@saeris.io> (https://github.com/Saeris/)",
  "license": "MIT",
  "keywords": [
    "unified",
    "rehype",
    "plugin",
    "rehype-plugin",
    "inline",
    "optimize",
    "svg",
    "image",
    "img",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/rehype-inline-svg.git"
  },
  "homepage": "https://github.com/saeris/rehype-inline-svg#readme",
  "bugs": {
    "url": "https://github.com/saeris/rehype-inline-svg/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=20.x"
  },
  "packageManager": "yarn@4.10.3",
  "type": "module",
  "main": "src/index.ts",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "module": "./dist/index.mjs",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "ci": "yarn lint && yarn typecheck && yarn test",
    "build": "tsdown",
    "lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "format": "yarn lint --fix",
    "release": "yarn build && changeset publish",
    "test": "vitest --fileParallelism",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "rehype-parse": "^9.0.1",
    "svgo": "^4.0.0",
    "unified": "^11.0.5",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@saeris/eslint-config": "^3.1.2",
    "@types/node": "^24.10.1",
    "@types/unist": "^3.0.3",
    "dedent": "^1.7.0",
    "eslint": "9.39.1",
    "prettier": "^3.6.2",
    "rehype-format": "^5.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "to-vfile": "^8.0.0",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vitest": "^3.2.3"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  }
}
